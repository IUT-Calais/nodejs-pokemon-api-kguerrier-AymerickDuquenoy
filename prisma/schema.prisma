generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Type {
  id   Int    @id @default(autoincrement())
  name String @unique
  pokemon Pokemon @relation(fields: [pokemonID], references: [id])
  pokemonID Int
}

model Pokemon{
  id Int @id @default(autoincrement())
  name String @unique  
  pokedexId Int @unique
  type Type[]
  lifePoints Int 
  size Float?
  weight Float?
  imageUrl String?
}
